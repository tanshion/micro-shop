<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.abc1236.ms.dao.MenuDAO">
    <select id="selectMenuByRoleId" resultType="com.abc1236.ms.entity.system.Menu">
        select *
        from t_sys_relation rel
                 inner join t_sys_menu m on rel.menuid = m.id
        where m.status = 1
          and rel.roleid = #{roleId}
    </select>
</mapper>